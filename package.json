{
  "name": "electron-with-express",
  "description": "Demonstrates spawning Express from Electron and using it to serve content",
  "version": "1.0.15",
  "main": "./dist/src/main.js",
  "icon": "./dist/favicon.ico",
  "expressPort": 3000,
  "basePath": "./dist",
  "authors": [
    "Frank Hale <frankhale@gmail.com>"
  ],
  "build": {
    "appId": "brandin.test",
    "files": [
      "./dist/**/*"
    ],
    "extends": null,
    "asarUnpack": "*.{node,dll,db}"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "pack": "tsc && set env DEBUG=electron-builder && npx electron-builder build",
    "start": "tsc && electron ."
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@types/node-fetch": "^2.6.11",
    "electron": "^28.2.2",
    "typescript": "^5.3.3"
  }
}
